. /etc/profile.d/modules.sh 

cd $PBS_O_WORKDIR 
 echo  $PBS_JOBID 
 mkdir $WORKDIR/$PBS_JOBID 
 ln -s $WORKDIR/$PBS_JOBID 
 cp * $WORKDIR/$PBS_JOBID  
 cd $WORKDIR/$PBS_JOBID 
 export NPROCS=`wc -l < $PBS_NODEFILE` 

export OMP_NUM_THREADS=4 


source ~/.python_mods
conda activate citam-env
python /home/reveilm2/CITAM/citam/citam/cli.py engine run sim_inputs.json

cp -r * $PBS_O_WORKDIR 

